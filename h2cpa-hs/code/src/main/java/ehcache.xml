<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache
		eternal="false"
		maxElementsInMemory="1000000"
		overflowToDisk="true"
		diskPersistent="false"
		timeToIdleSeconds="100000"
		timeToLiveSeconds="80000"
		memoryStoreEvictionPolicy="LRU" />
</ehcache>